  keywords = {overfishing,fisheries management,stock status,precautionary management,sustainable fishing},
  pages = {1040-1046},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/Hilborn and Ovando - 2014 - Reflections on the success of traditional fisherie2.pdf}
}

