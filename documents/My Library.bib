  author = {Fujita, Rod and Thornhill, Daniel J and Karr, Kendra and Cooper, Cara H and Dee, Laura E},
  year = {2013},
  keywords = {Aquarium trade,curio trade,data-poor fisheries,ornamental fisheries,productivity susceptibility analysis,sustainable fisheries management},
  pages = {n/a--n/a},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/Fujita et al. - 2013 - Assessing and managing data-limited ornamental fis.pdf;/Users/danovan/Zotero/storage/5VQ8UTN5/abstract\;jsessionid=F1539CA534ACFEF62D48412E4E75E7A6.html}
}

