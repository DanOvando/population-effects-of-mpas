  keywords = {Management strategy evaluation,Atlantis,ecological risk assessment,ecosystem-based fishery management,harvest strategy framework},
  pages = {633-639},
  file = {/Users/danovan/Zotero/storage/PBZHPIMZ/Smith et al. - 2007 - Scientific tools to support the practical implemen.pdf}
}

