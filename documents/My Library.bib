  pages = {1399-1409},
  file = {/Users/danovan/Google Drive/references/McGilliard et al. - 2011 - Spatial structure induced by marine reserves shape.pdf;/Users/danovan/Zotero/storage/W9A8MQA7/10-0001.html}
}

