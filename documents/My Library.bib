@article{yang,
  title = {How ``{{The Blob}}'' Affected Groundfish Distributions in the {{Gulf}} of {{Alaska}}},
  volume = {0},
  copyright = {\textcopyright{} 2019 John Wiley \& Sons Ltd},
  issn = {1365-2419},
  abstract = {We investigated the distributional shifts of groundfish in response to anomalous ocean conditions, particularly the recent anomalously warm period (2014\textendash{}2016; ``The Blob''), based on data from ten Gulf of Alaska bottom trawl surveys conducted by the Alaska Fisheries Science Center during 1996\textendash{}2015. Six groundfish species were considered: Pacific cod (Gadus macrocephalus), arrowtooth flounder (Atheresthes stomias), walleye pollock (Gadus chalcogrammus), Pacific ocean perch (Sebastes alutus), northern rock sole (Lepidopsetta polyxystra), and southern rock sole (Lepidopsetta bilineata). Ontogenetic differences were examined by dividing data for each fish species into size classes. Our study demonstrated that after accounting for size-specific depth preferences, the spatial responses of groundfish to anomalous ocean conditions differed by species and foraging guild in the central Gulf of Alaska. Pacific cod and arrowtooth flounder showed similar responses to ocean warming, but different responses to cooling. In general, Pacific cod moved to deeper depths in warmer years and moved to shallower depths in colder years. Arrowtooth flounder also moved deeper in warmer years. However, in colder years, large arrowtooth flounder ({$>$}40 cm) shifted toward shallower depths while smaller-sized fish shifted toward deeper depths. In warmer years, large pollock ({$>$}30 cm) moved to deeper waters while smaller pollock (10\textendash{}20 cm) moved to shallower waters. Pacific ocean perch exhibited an opposite response to thermal changes in habitat compared with Pacific cod and arrowtooth flounder. They moved deeper in colder years, but there was no clear change in depth as a function of size in response to warmer habitat.},
  language = {en},
  number = {0},
  journal = {Fisheries Oceanography},
  doi = {10.1111/fog.12422},
  author = {Yang, Qiong and Cokelet, Edward D. and Stabeno, Phyllis J. and Li, Lingbo and Hollowed, Anne B. and Palsson, Wayne A. and Bond, Nicholas A. and Barbeaux, Steven J.},
  keywords = {Pacific cod,Gulf of Alaska,walleye pollock,arrowtooth flounder,bottom trawl survey,groundfish distributional shift,large marine heatwave},
  file = {/Users/danovan/Google Drive/references/Yang et al. - How “The Blob” affected groundfish distributions i.pdf;/Users/danovan/Zotero/storage/P87K7C8G/fog.html}
}

