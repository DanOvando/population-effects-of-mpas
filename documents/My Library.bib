  keywords = {sustainability,Code of conduct,Rapid appraisal,Attribute scoring,Ethics,Multi-dimensional scaling,Multi-disciplinary,Ordination},
  pages = {255-270},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/Pitcher and Preikshot - 2001 - rapfish a rapid appraisal technique to evaluate t.pdf}
}

@article{mcclenachan2009,
