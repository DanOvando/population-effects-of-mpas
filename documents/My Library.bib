  year = {2015},
  keywords = {Fisheries,marine reserve,MPA,sharks,generalized linear mixed models,AMP,cero inflación,ciencia ciudadana,citizen science,eastern tropical Pacific,modelos lineales mixtos generalizados,Pacífico oriental tropical,reserva marina,tiburones,zero-inflated},
