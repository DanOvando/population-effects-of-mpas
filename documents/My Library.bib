  month = jan,
  year = {2007},
  keywords = {Management strategy evaluation,Atlantis,ecological risk assessment,ecosystem-based fishery management,harvest strategy framework},
  pages = {633-639},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/Smith et al2.pdf}
}

@article{halpern2003,
