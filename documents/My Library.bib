@article{larsen2019,
  title = {Causal Analysis in Control\textendash{}Impact Ecological Studies with Observational Data},
  volume = {0},
  copyright = {\textcopyright{} 2019 The Authors. Methods in Ecology and Evolution \textcopyright{} 2019 British Ecological Society},
  issn = {2041-210X},
  abstract = {Randomized experiments have long been the gold standard in determining causal effects in ecological control\textendash{}impact studies. However, it may be difficult to address many ecologically and policy-relevant control\textendash{}impact questions-such as the effect of forest fragmentation or protected areas on biodiversity through experimental manipulation due to scale, costs and ethical considerations. Yet, ecologists may still draw causal insights in observational control\textendash{}impact settings by exploiting research designs that approximate the experimental ideal. Here, we review the challenges of making causal inference in non-experimental control\textendash{}impact scenarios as well as a suite of statistical tools specifically designed to overcome such challenges. These tools are widely used in fields where experimental research is more limited (i.e., medicine, economics), and could be applied by ecologists across numerous sub-disciplines. Using hypothetical examples, we discuss why bias is likely to plague observational control\textendash{}impact studies in ways that do not surface with experimental manipulations, why bias is generally the barrier to causal inference, and different methods to overcome this bias. Satellite-, survey- and citizen\textendash{}science data hold great potential for advancing key questions in ecology that would otherwise be prohibitive to pursue experimentally. However, to harness such data to understand causal impacts of land, environmental and policy changes, we must expand our toolset such that we can improve inference and more confidently advance ecological understanding and science-informed policy.},
  language = {en},
  number = {0},
  journal = {Methods in Ecology and Evolution},
  doi = {10.1111/2041-210X.13190},
  author = {Larsen, Ashley E. and Meng, Kyle and Kendall, Bruce E.},
  year = {2019},
  keywords = {causal analyses,econometrics,before-after-control–impact,citizen science data,satellite data,science-informed policy,statistical ecology,survey data},
  file = {/Users/danovan/Google Drive/references/Larsen et al. - Causal analysis in control–impact ecological studi.pdf;/Users/danovan/Zotero/storage/VNIIY3I8/2041-210X.html}
}

@article{chape2005a,
