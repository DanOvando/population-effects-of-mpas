  journal = {Heredity},
  doi = {10.1038/s41437-017-0025-2},
  author = {Pazmi{\~n}o, Diana A. and Maes, Gregory E. and Green, Madeline E. and Simpfendorfer, Colin A. and {Hoyos-Padilla}, E. Mauricio and Duffy, Clinton J. A. and Meyer, Carl G. and Kerwath, Sven E. and {Salinas-de-Le{\'o}n}, Pelayo and van Herwerden, Lynne},
  month = jan,
  year = {2018},
