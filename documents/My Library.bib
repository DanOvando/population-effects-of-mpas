  month = jun,
  year = {2000},
  keywords = {marine reserves,spillover,Mediterranean,carrying capacity,density,fishery enhancement},
  pages = {144-158},
  file = {/Users/danovan/Google Drive/references/Lizaso et al. - 2000 - Density dependence in marine protected populations2.pdf;/Users/danovan/Zotero/storage/4LJE8RQD/92F5AF28F8406402673A4FCBCB776840.html}
