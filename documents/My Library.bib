  keywords = {Model,Growth,Simulation,Dipterocarp forest,Logging scenarios,Multicriteria decision analysis,Tropical rainforest},
  pages = {321-333},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/Huth et al. - 2004 - Multicriteria evaluation of simulated logging scen.pdf}
