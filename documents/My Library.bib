  author = {Brennan, Sean R. and Schindler, Daniel E. and Cline, Timothy J. and Walsworth, Timothy E. and Buck, Greg and Fernandez, Diego P.},
  month = may,
  year = {2019},
  pages = {783-786},
  file = {/Users/danovan/Google Drive/references/Brennan et al. - 2019 - Shifting habitat mosaics and fish production acros.pdf;/Users/danovan/Zotero/storage/YSIDP6NL/783.html},
  pmid = {31123135}
