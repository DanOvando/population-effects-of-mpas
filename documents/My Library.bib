  month = feb,
  year = {2008},
  keywords = {AIC,Bayes factor,BIC,DIC,Model selection},
  pages = {1-10},
  file = {/Users/danovan/Google Drive/references/Ward - 2008 - A review and comparison of four commonly used Baye.pdf;/Users/danovan/Zotero/storage/2NVRU3LQ/S0304380007005558.html}
