  year = {2006},
  keywords = {ecological modelling,habitat selection,minimum adequate model,multivariate statistical analysis,statistical bias},
  pages = {1182-1189},
  file = {/Users/danovan/Google Drive/references/Whittingham et al. - 2006 - Why do we still use stepwise modelling in ecology .pdf;/Users/danovan/Zotero/storage/QKLUH99U/j.1365-2656.2006.01141.html}
}

@article{schad2019,
