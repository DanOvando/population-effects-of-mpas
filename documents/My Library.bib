@article{gavaris1980,
  title = {Use of a {{Multiplicative Model}} to {{Estimate Catch Rate}} and {{Effort}} from {{Commercial Data}}},
  volume = {37},
  issn = {0706-652X},
  abstract = {The multiplicative model described by Robson can be used to obtain estimates of annual standardized catch rate and its variance by including years in the model. The corresponding estimate of standardized effort is the ratio of the total catch and the estimated catch rate. If the error is assumed to be lognormally distributed then these estimators have well defined statistical properties.Key words: effort standardization, catch rate, multiplicative model, stock assessment, Si l'on inclut les ann{\'e}es dans le mod{\`e}le multiplicateur d{\'e}crit par Robson, on peut s'en servir pour obtenir des estimations du taux de capture normalis{\'e} annuel et de sa variance. L'estimation correspondante de l'effort normalis{\'e} est le rapport de la capture total et du taux de capture estim{\'e}. En supposant que l'erreur a une distribution log normale, ces estimateurs ont des propri{\'e}t{\'e}s statistiques bien d{\'e}finies.},
