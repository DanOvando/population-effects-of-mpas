  month = jun,
  year = {2015},
  keywords = {Fisheries,environmental impacts,Sea ice,Animal behavior,Algorithms,Data management,Data processing,Ships},
  pages = {e0130746},
  file = {/Users/danovan/Zotero/storage/3H56EUE5/Natale et al. - 2015 - Mapping Fishing Effort through AIS Data.pdf}
}

