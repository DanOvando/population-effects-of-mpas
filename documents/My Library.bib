  journal = {Coral Reefs},
  doi = {10.1007/s00338-011-0812-9},
  author = {Teneva, Lida and Karnauskas, Mandy and Logan, Cheryl A. and Bianucci, Laura and Currie, Jock C. and Kleypas, Joan A.},
  month = mar,
  year = {2012},
  keywords = {Freshwater \& Marine Ecology,Oceanography,Coral reefs,Global Warming,Bleaching,Environmental variability,Sea surface temperature},
  pages = {1-12},
