  journal = {Ecological Applications},
  doi = {10.1890/04-1688},
  author = {{Rogers-Bennett}, Laura and Leaf, Robert T.},
  month = feb,
  year = {2006},
  keywords = {marine conservation,sustainability,deterministic matrix models,endangered species,growth transitions,population growth,prospective elasticity analysis},
  pages = {213-224},
