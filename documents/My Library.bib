  keywords = {fleet dynamics,behavioural responses,dynamic-state variable modelling,fishing effort,quota regulations},
  pages = {323-333},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/Poos et al. - 2010 - Individual quotas, fishing effort allocation, and .pdf;/Users/danovan/Zotero/storage/KVM5IE62/323.html}
