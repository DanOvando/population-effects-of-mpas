  month = sep,
  year = {2013},
  keywords = {social-ecological systems,Marine protected areas,Marine spatial planning,ecosystem-based management,environmental justice},
  pages = {6229-6234},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/Halpern et al. - 2013 - Achieving the triple bottom line in the face of in.pdf;/Users/danovan/Zotero/storage/T4GKKEV7/6229.html},
  pmid = {23530207}
