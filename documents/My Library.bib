  number = {4},
  journal = {Ecological Applications},
  doi = {10.1002/eap.1511},
  author = {Goetze, Jordan S. and {Januchowski-Hartley}, Fraser A. and Claudet, Joachim and Langlois, Tim J. and Wilson, Shaun K. and Jupiter, Stacy D.},
  month = jun,
  year = {2017},
  keywords = {fisheries management,conservation,artisanal fisheries,compliance,catch efficiency,customary management,fish behavior,flight initiation distance},
  pages = {1178-1189},
