@article{costello,
  title = {Removing Biases in Forecasts of Fishery Status},
  issn = {1387-6996, 1573-6989},
  abstract = {A recent highly cited paper from this journal develops a model predicting maximum sustainable yield ( MSYMSY ) of a fishery using the historical maximum catch ( MaxCatchMaxCatch ). The model is parameterized with a small sample of fisheries from the United States, and is subsequently applied globally to estimate the benefits of fishery recovery. That empirical relationship has been adopted for many subsequent high-profile analyses. Unfortunately, the analysis suffers from two important oversights: (1) because the model is non-linear, it suffers from ``retransformation bias'' and therefore the results significantly understate MSYMSY and (2) the analysis is parameterized from of a very limited data set and so generalizability of the fitted empirical relationship between MSYMSY and MaxCatchMaxCatch to global fisheries is questionable. Here, we rectify both oversights and provide an updated estimate of the relationship between MSYMSY and MaxCatchMaxCatch .},
  language = {en},
  journal = {Journal of Bioeconomics},
  doi = {10.1007/s10818-013-9158-4},
  author = {Costello, Christopher and Desch{\^e}nes, Olivier and Larsen, Ashley and Gaines, Steven},
  keywords = {Fisheries,maximum sustainable yield,Environmental Economics,Economic Theory,Behavioural Sciences,D57,Law and Economics,Q22,F01,Political Science; general,Retransformation bias},
  pages = {1-7},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/Costello et al. - Removing biases in forecasts of fishery status.pdf}
}

