  month = may,
  year = {2016},
  keywords = {economic objectives,ecosystem objectives,MEY,mixed-fisheries,operationalizing MSY,social objectives},
  pages = {fsw071},
  file = {/Users/danovando/SFG Work/General Library/Reference Library/Rindorf et al. - 2016 - Food for thought pretty good multispecies yield.pdf}
}

