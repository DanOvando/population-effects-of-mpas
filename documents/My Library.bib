  year = {2019},
  pages = {9142-9146},
  file = {/Users/danovan/Google Drive/references/Gephart et al. - 2019 - Opinion To create sustainable seafood industries,.pdf;/Users/danovan/Zotero/storage/5GVIXRFR/9142.html},
