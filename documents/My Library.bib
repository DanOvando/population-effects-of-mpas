  month = jun,
  year = {2012},
  keywords = {Bio-economic models,fisher behaviour,fishery modelling,fleet dynamics,Review},
  pages = {216-235},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/van Putten et al2.pdf}
}

