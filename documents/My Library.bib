  keywords = {Fish,transfer efficiency,food web,body mass,consumer–resource,feeding interaction,predator–prey,Size spectra,trophic level},
  pages = {222-232},
  file = {/Users/danovando/SFG Work/General Library/Reference Library/Barnes et al. - 2010 - Global patterns in predator–prey size relationship.pdf}
}

