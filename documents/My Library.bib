  month = sep,
  year = {2015},
  keywords = {Environmental Economics,Environmental Management,Economic Policy,Conservation policy,Economic incentives,Economics/Management Science; general,Environmental Law/Policy/Ecojustice,General synthetic inverse demand systems,Global tuna fisheries},
  pages = {1-16},
  file = {/Users/danovando/SFG Work/General Library/Reference Library/Sun et al. - 2015 - Fewer Fish for Higher Profits Price Response and .pdf}
