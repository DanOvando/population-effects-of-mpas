  keywords = {fisheries management,serial depletion,Stock synthesis,simulation testing,Spawning biomass,Stock structure},
  pages = {22-38},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/Cope and Punt - 2011 - Reconciling stock assessment and management scales.pdf}
}

