  keywords = {climate change,conservation,biogeography,Climate-change ecology},
  pages = {492-495},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/Burrows et al. - 2014 - Geographical limits to species-range shifts are su.pdf}
