  month = jan,
  year = {2012},
  keywords = {Co-management,Individual transferable quotas,Fishery cooperative association,Rights-based fishery management,territorial use rights in fisheries},
  pages = {241-245},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/Yagi et al. - 2012 - Applicability of Individual Transferable Quotas (I.pdf;/Users/danovan/Zotero/storage/DKPVJSFN/S0308597X1100100X.html}
