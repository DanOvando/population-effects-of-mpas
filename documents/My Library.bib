  keywords = {habitat,fisheries management,Marine reserves,simulated annealing,Connectivity,MPA,biogeography,reserve size,Channel Islands,ecological linkages},
  pages = {170-184},
  file = {/Users/danovando/SFG Work/General Library/Reference Library/Airamé et al. - 2003 - Applying Ecological Criteria to Marine Reserve Des.pdf}
