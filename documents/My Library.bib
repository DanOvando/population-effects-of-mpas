  doi = {10.1111/faf.12351},
  author = {Clavelle, Tyler and Lester, Sarah E. and Gentry, Rebecca and Froehlich, Halley E.},
  keywords = {seafood,disease,fishing,fish farm,fishmeal,mariculture},
  file = {/Users/danovan/Google Drive/references/Clavelle et al. - Interactions and management for the future of mari.pdf;/Users/danovan/Zotero/storage/6CIM3IBF/faf.html}
}

