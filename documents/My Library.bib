}

@article{wood,
  title = {Human Impacts Decouple a Fundamental Ecological Relationship\textemdash{{The}} Positive Association between Host Diversity and Parasite Diversity},
  volume = {0},
  copyright = {\textcopyright{} 2018 John Wiley \& Sons Ltd},
  issn = {1365-2486},
  abstract = {Human impacts on ecosystems can decouple the fundamental ecological relationships that create patterns of diversity in free-living species. Despite the abundance, ubiquity, and ecological importance of parasites, it is unknown whether the same decoupling effects occur for parasitic species. We investigated the influence of fishing on the relationship between host diversity and parasite diversity for parasites of coral reef fishes on three fished and three unfished islands in the central equatorial Pacific. Fishing was associated with a shallowing of the positive host-diversity\textendash{}parasite-diversity relationship. This occurred primarily through negative impacts of fishing on the presence of complex life-cycle parasites, which created a biologically impoverished parasite fauna of directly transmitted parasites resilient to changes in host biodiversity. Parasite diversity appears to be decoupled from host diversity by fishing impacts in this coral reef ecosystem, which suggests that such decoupling might also occur for parasites in other ecosystems affected by environmental change.},
  language = {en},
  number = {0},
  journal = {Global Change Biology},
  doi = {10.1111/gcb.14159},
  author = {Wood, Chelsea L. and Zgliczynski, Brian J. and Haupt, Alison J. and Guerra, Ana Sof{\'i}a and Micheli, Fiorenza and Sandin, Stuart A.},
  keywords = {coral reefs,biodiversity,biophysical coupling,disease,environmental change,fishing,host–parasite interactions,parasite},
