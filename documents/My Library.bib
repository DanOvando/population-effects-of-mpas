  year = {2016},
  keywords = {Food security,Fisheries,Ecosystem management,China,trophic cascades},
  pages = {201612722},
  file = {/Users/danovando/Papers and Projects/General Library/Reference Library/Szuwalski et al. - 2016 - High fishery catches through trophic cascades in C.pdf}
}

