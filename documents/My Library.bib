  keywords = {ecosystem-based management,Atlantis,ecosystem modelling,exploited marine ecosystems},
  pages = {171-188},
  file = {/Users/danovan/Zotero/storage/9AUTQF4H/Fulton et al. - 2011 - Lessons in modelling and management of marine ecos.pdf}
}

@article{nicholson2004,
