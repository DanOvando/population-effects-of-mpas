@article{erisman2015,
  title = {Fish Spawning Aggregations: Where Well-Placed Management Actions Can Yield Big Benefits for Fisheries and Conservation},
  copyright = {\textcopyright{} 2015 John Wiley \& Sons Ltd},
  issn = {1467-2979},
  shorttitle = {Fish Spawning Aggregations},
  abstract = {Marine ecosystem management has traditionally been divided between fisheries management and biodiversity conservation approaches, and the merging of these disparate agendas has proven difficult. Here, we offer a pathway that can unite fishers, scientists, resource managers and conservationists towards a single vision for some areas of the ocean where small investments in management can offer disproportionately large benefits to fisheries and biodiversity conservation. Specifically, we provide a series of evidenced-based arguments that support an urgent need to recognize fish spawning aggregations (FSAs) as a focal point for fisheries management and conservation on a global scale, with a particular emphasis placed on the protection of multispecies FSA sites. We illustrate that these sites serve as productivity hotspots \textendash{} small areas of the ocean that are dictated by the interactions between physical forces and geomorphology, attract multiple species to reproduce in large numbers and support food web dynamics, ecosystem health and robust fisheries. FSAs are comparable in vulnerability, importance and magnificence to breeding aggregations of seabirds, sea turtles and whales yet they receive insufficient attention and are declining worldwide. Numerous case-studies confirm that protected aggregations do recover to benefit fisheries through increases in fish biomass, catch rates and larval recruitment at fished sites. The small size and spatio-temporal predictability of FSAs allow monitoring, assessment and enforcement to be scaled down while benefits of protection scale up to entire populations. Fishers intuitively understand the linkages between protecting FSAs and healthy fisheries and thus tend to support their protection.},
  language = {en},
  journal = {Fish and Fisheries},
  doi = {10.1111/faf.12132},
  author = {Erisman, Brad and Heyman, William and Kobara, Shinichi and Ezer, Tal and Pittman, Simon and {Aburto-Oropeza}, Octavio and Nemeth, Richard S},
  month = oct,
  year = {2015},
  keywords = {fisheries management,marine conservation,fish spawning aggregations,fisheries comanagement,marine productivity hotspots,physical–biological coupling},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/Erisman et al. - 2015 - Fish spawning aggregations where well-placed mana.pdf}
}

