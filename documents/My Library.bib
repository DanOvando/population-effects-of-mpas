  keywords = {protected areas,Conservation planning,Marine reserves,fishing effort,Marxan,áreas protegidas,biodiversidad marina,conservation costs,costos de conservación,esfuerzo de pesca,fishing exclusion zones,marine biodiversity,planificación de la conservación,reservas marinas,zonas de exclusión de pesca},
  pages = {691-700},
  file = {/Users/danovando/SFG Work/General Library/Reference Library/Klein et al. - 2008 - Striking a Balance between Biodiversity Conservati.pdf}
