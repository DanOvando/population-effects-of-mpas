  month = sep,
  year = {2017},
  keywords = {natural resource management,Bayesian variance function regression,diversity-stability relationship,ecological portfolio effects,income variability},
  pages = {201702506},
  file = {/Users/danovan/Google Drive/references/Anderson et al. - 2017 - Benefits and risks of diversification for individu.pdf;/Users/danovan/Zotero/storage/APEKEW4I/1702506114.html},
  pmid = {28923938}
