  year = {2003},
  pages = {1769-1784},
  file = {/Users/danovan/Google Drive/references/Friedlander et al. - 2003 - Designing Effective Marine Protected Areas in Seaf.pdf;/Users/danovan/Zotero/storage/FNSNJHB7/j.1523-1739.2003.00338.html}
