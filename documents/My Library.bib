@article{barnes2010,
  title = {Global Patterns in Predator\textendash{}Prey Size Relationships Reveal Size Dependency of Trophic Transfer Efficiency},
  volume = {91},
  issn = {1939-9170},
  abstract = {Predator\textendash{}prey body size relationships influence food chain length, trophic structure, transfer efficiency, interaction strength, and the bioaccumulation of contaminants. Improved quantification of these relationships and their response to the environment is needed to parameterize food web models and describe food web structure and function. A compiled data set comprising 29{$\mkern1mu$}582 records of individual prey eaten at 21 locations by individual predators that spanned 10 orders of magnitude in mass and lived in marine environments ranging from the poles to the tropics was used to investigate the influence of predator size and environment on predator and prey size relationships. Linear mixed effects models demonstrated that predator\textendash{}prey mass ratios (PPMR) increased with predator mass. The amount of the increase varied among locations and predator species and individuals but was not significantly influenced by temperature, latitude, depth, or primary production. Increases in PPMR with predator mass implied nonlinear relationships between log body mass and trophic level and reductions in transfer efficiency with increasing body size. The results suggest that very general rules determine dominant trends in PPMR in diverse marine ecosystems, leading to the ubiquity of size-based trophic structuring and the consistency of observed relationships between the relative abundance of individuals and their body size.},
  language = {en},
  number = {1},
  journal = {Ecology},
  doi = {10.1890/08-2061.1},
  author = {Barnes, Carolyn and Maxwell, David and Reuman, Daniel C. and Jennings, Simon},
  month = jan,
