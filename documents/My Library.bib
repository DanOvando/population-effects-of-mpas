  month = mar,
  year = {2016},
  keywords = {bioeconomic model,Rights-based fishery management,fishery recovery,fishery reform},
  pages = {5125-5129},
  file = {/Users/danovando/SFG Work/General Library/Reference Library/Costello et al. - 2016 - Global fishery prospects under contrasting managem.pdf},
  pmid = {27035953}
