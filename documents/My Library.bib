  month = dec,
  year = {2017},
  keywords = {Fisheries,Agent Based Modeling,Destructive Fishing Methods,Individual Time Preferences,Natural Resource Extraction,Time Discounting},
  pages = {91-103},
  file = {/Users/danovando/Reference Library/Javaid et al. - 2017 - When Patience Leads to Destruction The Curious Ca.pdf}
}

@article{zeller,
